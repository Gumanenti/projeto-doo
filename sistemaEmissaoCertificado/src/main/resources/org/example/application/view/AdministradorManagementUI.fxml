      <Button fx:id="btnDelete" layoutX="182.0" layoutY="344.0" mnemonicParsing="false" onAction="#deleteAdministrador" text="Deletar" />
      <Button fx:id="btnBack" layoutX="43.0" layoutY="344.0" mnemonicParsing="false" onAction="#backToPrewview" text="Voltar" />
   </children>
</AnchorPane>
